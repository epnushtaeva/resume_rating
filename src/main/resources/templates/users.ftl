<#import "/spring.ftl" as spring/>

<!DOCTYPE HTML>
<html>
   <head>
      <meta charset="UTF-8" />
      <title>Оценка резюме</title>
      <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no, minimal-ui">
      <link href="/css/font.css" rel="stylesheet">
      <link href="/css/materialdesignicons.min.css" rel="stylesheet">
      <link href="/css/vuetify.min.css" rel="stylesheet">
      <link href="/css/main.css" rel="stylesheet">
      <link href="/css/users.css" rel="stylesheet">
      <script src="/js/vue.js"></script>
      <script src="/js/vuetify.js"></script>
      <script src="/js/axios.min.js"></script>
      <script>
              document.addEventListener('DOMContentLoaded', function(){
               new Vue({
                    el: '#toolbar',
                    vuetify: new Vuetify(),
                  });

               new Vue({
                    el: '#all-specialities',
                    vuetify: new Vuetify(),
                    data: {
                         addUserDialog : false,
                         addUserFormValid: true,
                         editUserFormValid: true,
                         login: '',
                         fullName: '',
                         roleId: 0,
                         postId: 0,
                         roles: [],
                         posts: [],
                         filterPosts: [],
                         filterRoles: [],
                         add: false,
                         serverItemsLength:0,
                         currentAvatarPath: '',
                         users: [],
                         items: ['На данный момент есть открытые вакансии', 'На данный момент нет открытых вакансий'],
                         headers: [
                           {
                                                                                                           text: '№ п/п',
                                                                                                           align: 'left',
                                                                                                           sortable: false,
                                                                                                           value: 'rowNumber'
                                                                                        },
                                                                                        {
                                                                                                                                                                                                   text: 'Аватар',
                                                                                                                                                                                                   sortable: false,
                                                                                                                                                                                                   value: 'avatar'
                                                                                                                                                                                },
                                  {
                                                                                  text: 'Логин',
                                                                                  align: 'left',
                                                                                  sortable: false,
                                                                                  value: 'username'
                                                               },
                                                               {
                                                                                                                                                                          text: 'ФИО',
                                                                                                                                                                          align: 'left',
                                                                                                                                                                          sortable: false,
                                                                                                                                                                          value: 'fullName'
                                                                                                                                                       },
                                                                                                                                                       {
                                                                                                                                                                                                                                                                                                                                 text: 'Email',
                                                                                                                                                                                                                                                                                                                                 align: 'left',
                                                                                                                                                                                                                                                                                                                                 sortable: false,
                                                                                                                                                                                                                                                                                                                                 value: 'email'
                                                                                                                                                                                                                                                                                                              },
                                                                                                                                                                                                                                                                                                              {
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               text: 'Телефон',
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               align: 'left',
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               sortable: false,
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               value: 'phone'
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            },
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            {
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           text: 'Должность',
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           align: 'left',
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           sortable: false,
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           value: 'post'
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        },
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        {
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           text: 'Роль',
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           align: 'left',
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           sortable: false,
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           value: 'role'
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        },
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         {
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   text: 'Заходил последний раз',
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   align: 'left',
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   sortable: false,
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   value: 'lastLogin'
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                },

                                  { text: '', value: 'action', sortable: false }
                               ],
                        removeDialog:false,
                        loading:true,
                        options:{
                                  rowsPerPage:5,
                                  load: true,
                                  page: 1
                                 },
                                 editUserFormValid: true,
                         editingFullName: '',
                         editingLogin: '',
                         tempAvatarFile: null,
                         editingPassword: '',
                         checkPassword:'',
                         editingRoleId:3,
                         editingPostId:3,
                         editingPhone: '',
                         editingEmail: '',
                         errAvatar: '',

                         fullNameValidationRules: [
                                                                              value => !!value || "ФИО не может быть пустым"
                                                                           ],
                         loginValidationRules:[
                            value => !!value || "Логин не может быть пустым"
                         ],
                         editCheckPasswordValidationRules: [],
                         passwordValidationRules: [
                                                                                                  value => !!value || "Пароль не может быть пустым",
                                                                                                  value => (value && value.length >= 8) || "Пароль не может быть меньше 8 символов"
                                                                                                ],
                         checkPasswordValidationRules:[
                           value => !!value || "Пожалуйста, заполните это поле"
                         ],
                         emailValidationRules: [
                           value => (!value ||  /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/.test(value.toLowerCase())) || "Неверный формат email"
                         ],
                         editing: false,
                         removing: false,
                         userId: 0,
                         edit: false,
                         editUserDialog: false,
                         errMess: '',
                         errDialog: false,
                         tempAvatarFile: null,
                         currentAvatarPath: '',
                         loadingAvatar: false
                    },
                    watch: {
                            options: {
                                handler() {
                                    this.getDataFromApi();
                                },
                                deep: true
                            }
                   },
                    mounted () {
                              this.getDataFromApi();
                              this. initRoles();
                              this.initPosts();
                    },
                    methods:{
                            initRoles(){
                               fetch('/posts/all', {
                                                                                                                                                      method:'GET'
                                                                                                                                                     })
                                                                                                                                                     .then(res => res.json())
                                                                                                                                                     .then(res => {
                                                                                                                                                                   this.posts = res;
                                                                                                                                                                   let self = this;
                                                                                                                                                                   this.filterPosts = [];
                                                                                                                                                                   this.filterPosts.push({id: 0, name: ''});

                                                                                                                                                                   res.forEach(function(post){
                                                                                                                                                                     self.filterPosts.push({id: post.id, name: post.name});
                                                                                                                                                                   });
                                                                                                                                                                   console.log(this.posts);
                                                                                                                                                                  })
                                                                                                                                                     .catch(e => {

                                                                                                                                                                    console.error(JSON.stringify(e.message));
                                                                                                                                                                  });
                            },
                            initPosts(){
                             fetch('/roles/all', {
                                                                                                                                                                                   method:'GET'
                                                                                                                                                                                  })
                                                                                                                                                                                  .then(res => res.json())
                                                                                                                                                                                  .then(res => {
                                                                                                                                                                                                this.roles = res;
                                                                                                                                                                                                let self = this;
                                                                                                                                                                                                this.filterRoles = [];
                                                                                                                                                                                                                                                                                                                                                                  this.filterRoles.push({id: 0, russianName: ''});

                                                                                                                                                                                                                                                                                                                                                                  res.forEach(function(post){
                                                                                                                                                                                                                                                                                                                                                                    self.filterRoles.push({id: post.id, russianName: post.russianName});
                                                                                                                                                                                                                                                                                                                                                                  });
                                                                                                                                                                                                                                                                                                                                                                  })
                                                                                                                                                                                  .catch(e => {

                                                                                                                                                                                                 console.error(JSON.stringify(e.message));
                                                                                                                                                                                               });
                            },
                            getDataFromApi () {

                                    this.loading = true;

                                    let fullName = this.fullName;
                                    let login = this.login;
                                    let roleId = this.roleId;
                                    let postId = this.postId;
                                    const { page, rowsPerPage } = this.options;


                                           let dataTableOptions = {
                                            page : page,
                                            rowsPerPage : rowsPerPage,
                                            fullName: fullName,
                                            username: login,
                                            roleId: roleId,
                                            postId: postId
                                           };

                                           axios({
                                                          url: '/users/all',
                                                          method:'post',
                                                          data: dataTableOptions
                                                 })
                                                 .then(response=>{
                                                          if(response.status == 200){ console.log(response.data.data);

                                                          console.log( response.data.data);
                                                            this.users = response.data.data;

                                                            this.serverItemsLength  = response.data.totalRecords;
                                                            this.loading = false;

                                                          }
                                                 })
                                                 .catch(error=>{
                                                          if(error.response){

                                                          }
                                                 })
                                                 .finally(() => this.loading = false);
                            },
                            showAddDialog(){
                            this.errAvatar = "";
                            this.tempAvatarFile = null;
                            this.userId = 0 ;
                            this.currentAvatarPath = '';
                            this.addUserDialog = true;
                              this.editingLogin = '';
                                                             this.editingFullName = '';
                                                             this. editingPhone = '';
                                                             this. editingEmail = '';
                                                             this.editingPassword = '';
                                                             this.checkPassword = '';
                            },
                            searchInTable(){
                               this.getDataFromApi();
                            },
                            removeUser(){
                                let userId = this.userId;
                                this.removing = true;

                                                                fetch('/users/remove?userId='+userId, {
                                                                                                                       method:'POST'
                                                                                                                      })
                                                                                                                      .then(res => res.json())
                                                                                                                      .then(res => {
                                                                                                                                    this.removing = false;
                                                                                                                                    this.removeDialog = false;
                                                                                                                                    console.log(res);
                                                                                                                                      this.options.page = 1;
                                                                                                                                      this.getDataFromApi();
                                                                                                                                   })
                                                                                                                      .catch(e => {
                                                                                                                                     this.removing = false;
                                                                                                                                     console.error(JSON.stringify(e.message));
                                                                                                                                   });
                            },
                            addUser(){
                            if (this.$refs.form.validate()) {
                             if(this.editingPassword != this.checkPassword){
                              this.errDialog = true;
                             return;
                                                        }

                            this.add = true;
                            let login = this.editingLogin;
                            let fullName = this.editingFullName;
                            let roleId = this. editingRoleId;
                            let postId = this. editingPostId;
                            let phone = this. editingPhone;
                            let email = this. editingEmail;
                            let password = this.editingPassword;

                              axios({
                                                                                      url: '/users/add',
                                                                                      method:'post',
                                                                                      data: {
                                                                                       username: login,
                                                                                       fullName: fullName,
                                                                                       roleId: roleId,
                                                                                       postId: postId,
                                                                                       phone: phone,
                                                                                       email: email,
                                                                                       newPassword: password
                                                                                      }
                                                                             })
                                                                             .then(response=>{
                                                                                     this.add = false;

                                                                                     if(!response.data.hrFullName){
                                                                                      this.errMess = '';

                                                                            if(this.tempAvatarFile != null){
                                                                               let formData = new FormData();
                                                                                                                  formData.append("avatar", this.tempAvatarFile);


                                                                                                                  axios({
                                                                                                                                                                                                       url: '/users/loadAvatar/' + response.data.hrId,
                                                                                                                                                                                                       method:'post',
                                                                                                                                                                                                       data: formData
                                                                                                                                                                                              })
                                                                                                                                                                                              .then(response=>{
                                                                                                                                                                                              })
                                                                                                                                                                                              .catch(error=>{
                                                                                                                                                                                                       if(error.response){
                                                                                                                                                                                                         this.errAvatar = "Ошибка сервера";
                                                                                                                                                                                                       }
                                                                                                                                                                                              }).finally(()=>{
                                                                                                                                                                                               this.options.page = 1;
                                                                                                                                                                                                                                                                                                                                                                                                                          this.getDataFromApi();
                                                                                                                                                                                                                                                                                                                                                                                                                          this.addUserDialog = false;

                                                                                                                                                                                              });
                                                                            } else {
                                                                             this.options.page = 1;
                                                                                                                                                                                                                                                                                                        this.getDataFromApi();
                                                                                                                                                                                                                                                                                                        this.addUserDialog = false;

                                                                            }

                                                                             } else {
                                                                                 this.errMess = response.data.hrFullName;
                                                                              }
                                                                             })
                                                                             .catch(error=>{
                                                                                      if(error.response){
                                                                                         this.add = false;
                                                                                      }
                                                                             })
                                                                             .finally(() =>  this.add = false);
                                                                             }
                            },
                            showRemoveDialog(userId){
                              this.userId = userId;
                              this.removeDialog = true;
                            },
                            showEditDialog(item){
                                 this.currentAvatarPath = item.avatar;
                                 this.editUserDialog = true;
                                 this.editingLogin = item.username;
                                 this.editingFullName = item.fullName;
                                 this. editingRoleId = item.roleId;
                                 this. editingPostId = item.postId;
                                 this. editingPhone = item.phone;
                                 this. editingEmail = item.email;
                                 this.editingPassword = '';
                                 this.checkPassword = '';
                                 this.userId = item.id;
                                 this.errAvatar = "";
                            },
                            loadAvatar(event){
                               this.errAvatar = "";

                              if(event){
                                 let fileExtension = event.name.split('.');
                                 fileExtension = fileExtension[fileExtension.length - 1].toLowerCase();

                                 if(fileExtension!='jpg' && fileExtension != 'png'){
                                    this.errAvatar = "Недопустимый формат файла. Допустимые форматы: jpg, png.";
                                    return;
                                 }

                                if(this.userId == 0){
                                     this.tempAvatarFile = event;
                                } else {
                                    let formData = new FormData();
                                    formData.append("avatar", event);

                                    this.loadingAvatar = true;

                                    axios({
                                                                                                                         url: '/users/loadAvatar/' + this.userId,
                                                                                                                         method:'post',
                                                                                                                         data: formData
                                                                                                                })
                                                                                                                .then(response=>{ console.log(response);
                                                                                                                       this.loadingAvatar = false;
                                                                                                                       this.currentAvatarPath = response.data.hrFullName;
                                                                                                                       let id = this.userId;

                                                                                                                      this.users.forEach(function(item){
                                                                                                                                                                                                                                                                                                                                                                                         if(item.id == id){
                                                                                                                                                                                                                                                                                                                                                                                            item.avatar = response.data.hrFullName;
                                                                                                                                                                                                                                                                                                                                                                                         }
                                                                                                                                                                                                                                                                                                                                                                                         });
                                                                                                                })
                                                                                                                .catch(error=>{
                                                                                                                         this.loadingAvatar = false;
                                                                                                                         if(error.response){
                                                                                                                           this.errAvatar = "Ошибка сервера";
                                                                                                                         }
                                                                                                                })
                                                                                                                .finally(() =>   this.loadingAvatar = false);
                                }
                              }
                            },
                            editUser(){
                             if(this.$refs.form1.validate()){
                                if(this.editingPassword != this.checkPassword){
                                this.errDialog = true;
                                                          return;
                                                                                     }
                             this.edit = true;
                                                       let login = this.editingLogin;
                                                                                   let fullName = this.editingFullName;
                                                                                   let roleId = this. editingRoleId;
                                                                                   let postId = this. editingPostId;
                                                                                   let phone = this. editingPhone;
                                                                                   let email = this. editingEmail;
                                                                                   let id = this.userId;
                                                                                   let password = this.editingPassword;

                                                          axios({
                                                                                                                  url: '/users/update',
                                                                                                                  method:'post',
                                                                                                                  data: {
                                                                                                                   login: login,
                                                                                                                                                                                                         fullName: fullName,
                                                                                                                                                                                                         roleId: roleId,
                                                                                                                                                                                                         postId: postId,
                                                                                                                                                                                                         phone: phone,
                                                                                                                                                                                                         email: email,
                                                                                                                   id: id,

                                                                                                                                                                                                          newPassword: password
                                                                                                                  }
                                                                                                         })
                                                                                                         .then(response=>{

                                                                                                                 this.edit = false;
                                                                                                                 let posts = this.posts;
                                                                                                                 let roles = this.roles;
                                                                                                                  this.users.forEach(function(item){
                                                                                                                                                                                                                                                                   if(item.id == id){
                                                                                                                                                                                                                                                                      item.fullName = fullName;
                                                                                                                                                                                                                                                                                                                                                                                                                                                                              item.roleId = roleId;
                                                                                                                                                                                                                                                                                                                                                                                                                                                                              item.postId = postId;
                                                                                                                                                                                                                                                                                                                                                                                                                                                                              item.phone = phone;
                                                                                                                                                                                                                                                                                                                                                                                                                                                                              item.email = email;
                                                                                                                                                                                                                                                                                                                                                                                                                                                                              posts.forEach(function(post){if(post.id == item.postId){item.post = post.name;}});
                                                                                                                                                                                                                                                                                                                                                                                                                                                                              roles.forEach(function(post){if(post.id == item.roleId){item.role = post.russianName;}});


                                                                                                                                                                                                                                                                   }
                                                                                                                                                                                                                                                                 });

                                                                                                                                                                                                                                                        this.editUserDialog = false;
                                                                                                         })
                                                                                                         .catch(error=>{
                                                                                                                  if(error.response){
                                                                                                                     this.edit = false;
                                                                                                                  }
                                                                                                         })
                                                                                                         .finally(() =>  this.edit = false);
                                                                                                         }
                            }
                    }
               });
          });
      </script>
   </head>
   <body>
